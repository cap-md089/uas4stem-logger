{
    "compilerOptions": {
        "target": "es5",
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "alwaysStrict": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "pretty": false,
        "removeComments": true,
        "lib": [
            "dom",
            "es2015",
            "es5",
            "es6"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}